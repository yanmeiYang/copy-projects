{
  "private": true,
  "name": "aminer2b",
  "version": "1.3.0",
  "scripts": {
    "rhv": "roadhog --version",
    "dev": "roadhog --version && roadhog dev",
    "devm": "roadhog --version && node --max_old_space_size=4096 node_modules/roadhog/lib/scripts/realDev.js",
    "devold": "cross-env BROWSER=none HOST=0.0.0.0 roadhog dev",
    "lint": "eslint --fix --ext .js src",
    "build": "roadhog build",
    "build:a": "roadhog build --analyze",
    "build:ab": "node --max_old_space_size=4096 node_modules/roadhog/lib/build.js --analyze",
    "build:dll": "roadhog buildDll",
    "build:new": "node version && roadhog build",
    "build:deploy": "node bin/build.js -s ${system} && roadhog build",
    "build:deployxxx": "node bin/build.js -s ${system} && node version && roadhog build"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.36",
    "antd": "^2.6.4",
    "dva": "^2.1.0-0",
    "dva-loading": "^0.3.0",
    "history": "^4.7.2",
    "less-vars-to-js": "^1.2.1",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.36",
    "webpack": "^3.10.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "eslint": "^4.11.0",
    "eslint-config-af-react": "^0.3.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "husky": "^0.13.0",
    "redbox-react": "^1.5.0"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.3"
  },
  "pre-commit": [
    "lint"
  ]
}
